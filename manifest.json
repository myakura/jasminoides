{
	"manifest_version": 2,
	"name": "Jasminoides",
	"description": "Open Gemini in sidebar",
	"version": "0.0.5",
	"permissions": ["<all_urls>"],
	"sidebar_action": {
		"default_panel": "jasminoides_sidebar.html",
		"default_title": "Jasminoides",
		"open_at_install": false
	},
	"background": {
		"scripts": ["jasminoides_background.js"],
		"persistent": false,
		"type": "module"
	},
	"commands": {
		"toggle-sidebar": {
			"description": "Toggle sidebar",
			"suggested_key": {
				"default": "Alt+Shift+G"
			}
		}
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "{0b071dbd-911a-4f41-9bc7-b5f276ff93de}",
			"strict_min_version": "117.0"
		}
	}
}
